  {{#each metatags.og}}
    <meta property="{{this.property}}" content="{{this.content}}">
  {{/each}}


  {{#each metatags.standard}}
    <meta name="{{this.name}}" content="{{this.content}}">
  {{/each}}
  <link rel="alternate" type="application/rss+xml" href="/feed/rss.xml">
</head>
<body class="js-theme theme{{#if section }} theme-{{section}}{{/if}}">
  <div id="body">
    <div id="main">
      <a id="jump" href="#main-h1">Jump to content</a>
      </header>
      {{> "global/navbar/navbar"}}
      <main id="main-content" class="site-content">
        {{> "global/breadcrumbs/breadcrumbs"}}
        <div class="l-site_width">
          {{#if contents}}
{{> "content/text_container/text_container" content=contents}} {{!-- needs to have zero space indenting for code examples --}}
          {{/if}}
          {{#if subPages}}
            {{> "content/cardmenu/cardmenu" menuitem=subPages}}
          {{/if}}
        </div>

        {{> "content/prev_next/prev_next" prev=previousPage next=nextPage}}
      </main>
      {{> "global/footer/footer"}}
    </div>
  </div>
  <script src="/js/vendor.js"></script>
  <script src="/js/scripts.js"></script>
</body>
</html>
